+{
+	unsigned int i = 0;
+	
+	if (strlen(mac) != ETH_ALEN*3-1)
+		xtables_error(PARAMETER_PROBLEM, "Bad mac address \"%s\"", mac);
+	
+	for (i = 0; i < ETH_ALEN; i++) {
+		long number;
+		char *end;
+		
+		number = strtol(mac + i*3, &end, 16);
+		
+		if (end == mac + i*3 + 2
+				&& number >= 0
+				&& number <= 255)
+		{
+			info->macaddr[i] = number;
+		}
