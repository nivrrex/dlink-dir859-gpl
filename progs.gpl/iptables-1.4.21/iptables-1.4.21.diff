diff -Nru ori/iptables-1.4.21/extensions/libxt_mport.c iptables-1.4.21/extensions/libxt_mport.c
--- ori/iptables-1.4.21/extensions/libxt_mport.c	1970-01-01 08:00:00.000000000 +0800
